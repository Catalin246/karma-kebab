{
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "PostgreSQLEntityFramework": "Server=karama-kebab-network;Port=5432;Database=employeedb;Username=postgres;Password=password;"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:3005"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "PostgreSQLEntityFramework": "Server=karama-kebab-network;Port=5432;Database=employeedb;Username=postgres;Password=password;"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:3005;http://localhost:3007"
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "PostgreSQLEntityFramework": "Server=karama-kebab-network;Port=5432;Database=employeedb;Username=postgres;Password=password;"
      }
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "8081",
        "ASPNETCORE_HTTP_PORTS": "8080"
      },
      "publishAllPorts": true,
      "useSSL": true
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:47495",
      "sslPort": 44340
    }
  }
}